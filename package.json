{
  "name": "warp-contracts-pubsub",
  "version": "1.0.6",
  "description": "Pub/sub library for warp contracts",
  "types": "./lib/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "exports": {
    ".": "./lib/cjs/index.js",
    "./streamr": "./lib/cjs/streamr/index.js",
    "./appsync": "./lib/cjs/appsync/index.js"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=16.5"
  },
  "scripts": {
    "build:cjs": "tsc -b tsconfig.json",
    "build:types": "tsc -b tsconfig.types.json",
    "build": "yarn run clean && yarn build:cjs && yarn build:types",
    "build:watch": "nodemon -e js,ts --watch src --exec \"yarn build\"",
    "format": "prettier --write 'src/**/*.ts'",
    "clean": "rimraf ./lib",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepublishOnly": "yarn lint",
    "preversion": "yarn lint && yarn build",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "yalc:publish": "yarn build && yalc publish --push",
    "test": "jest",
    "test:unit": "jest ./src/__tests__/unit"
  },
  "license": "MIT",
  "files": [
    "lib/",
    "README.md"
  ],
  "dependencies": {
    "@aws-amplify/api": "^5.0.19",
    "@aws-amplify/core": "^5.1.2",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
